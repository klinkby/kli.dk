{
    "routes": [
        {
            "route": "/sitemap.xml",
            "redirect": "/index.xml",
            "statusCode": 301
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/404/index.html"
        }
    },
    "globalHeaders": {
        "content-security-policy": "default-src https: 'self'; style-src: https: 'self' 'unsafe-inline';  object-src 'none'; frame-src 'self'; img-src 'self'",
        "X-Frame-Options": "SAMEORIGIN",
        "X-Content-Type-Options": "nosniff",
        "x-xss-protection": "1; mode=block",
        "referrer-policy": "same-origin"
    }
}